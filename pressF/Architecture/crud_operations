from app_name.models import User
user = User.objects.create(username='new_user', email='user@example.com', password='pass123')
user = User.objects.get(id=1) # cоздаём пользователя
print(user.username)
user = User.objects.get(id=1) 
print(user.username) # получить имя пользователя
user.username = 'updated_user' # обновляем пользователя
user.save() # сохраняем
user.delete() # удаляем пользователя