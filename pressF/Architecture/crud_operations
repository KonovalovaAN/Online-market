python manage.py create "new_user" "new_user@example.com" "password123" # Запуск программы через консоль
python manage.py create "username" "email@example.com" "password"  # Создание пользователя
python manage.py read # Чтение всех пользователей
python manage.py read 1 # Чтение одного пользователя по ID
python manage.py update 1 "new_username" "new_email@example.com" "new_password" # Обновление пользователя по ID
python manage.py delete 1 # Удаление пользователя по ID
